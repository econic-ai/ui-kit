{
	"name": "@econic/site-kit",
	"version": "1.0.0",
	"sideEffects": false,
	"description": "Styles and components for use in *.econic.ai websites",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"check": "svelte-kit sync && svelte-check",
		"package": "svelte-kit sync",
		"lint": "prettier --check .",
		"format": "prettier --write ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/econic-ai/ui-libs.git"
	},
	"author": "Jordan Rancie",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/econic-ai/ui-libs/issues"
	},
	"homepage": "https://github.com/econic-ai/ui-libs/tree/main/site-kit#readme",
	"dependencies": {
		"@codemirror/autocomplete": "^6.9.0",
		"@codemirror/commands": "^6.2.5",
		"@codemirror/lang-css": "^6.2.1",
		"@codemirror/lang-html": "^6.4.6",
		"@codemirror/lang-javascript": "^6.2.1",
		"@codemirror/language": "^6.9.0",
		"@codemirror/lint": "^6.4.1",
		"@codemirror/search": "^6.5.2",
		"@codemirror/state": "^6.5.0",
		"@codemirror/view": "^6.35.3",
		"@fontsource/inter": "^5.1.1",
		"@lezer/common": "^1.0.4",
		"@replit/codemirror-lang-svelte": "^6.0.0",
		"@shikijs/twoslash": "^1.22.0",
		"autoprefixer": "^10.4.18",
		"esm-env": "^1.0.0",
		"json5": "^2.2.3",
		"postcss": "^8.4.35",
		"sass": "^1.72.0",
		"shiki": "^1.22.0"
	},
	"devDependencies": {
		"@sveltejs/kit": "^2.12.1",
		"@types/node": "^20.12.11",
		"flexsearch": "^0.7.43",
		"magic-string": "^0.30.11",
		"marked": "^14.1.2",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.3.2",
		"svelte": "5.14.0",
		"svelte-check": "^4.1.1",
		"typescript": "^5.5.4",
		"vite": "^5.4.3"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^2.12.1",
		"svelte": "^4.0.0 || ^5.0.0-next.1"
	},
	"types": "./dist/types.d.ts",
	"exports": {
		".": {
			"types": "./src/lib/types.d.ts"
		},
		"./actions": {
			"default": "./src/lib/actions/index.ts"
		},
		"./state": {
			"default": "./src/lib/state/index.ts"
		},
		"./stores": {
			"default": "./src/lib/stores/index.ts"
		},
		"./codemirror": {
			"default": "./src/lib/codemirror/index.js"
		},
		"./branding/svelte-logo.svg": "./src/lib/branding/svelte-logo.svg",
		"./components": {
			"default": "./src/lib/components/index.ts",
			"svelte": "./src/lib/components/index.ts"
		},
		"./docs": {
			"default": "./src/lib/docs/index.ts",
			"svelte": "./src/lib/docs/index.ts"
		},
		"./markdown": {
			"default": "./src/lib/markdown/index.ts"
		},
		"./markdown/preprocess": {
			"default": "./src/lib/markdown/preprocess.ts"
		},
