			"default": "./src/lib/nav/index.ts",
			"svelte": "./src/lib/nav/index.ts"
		},
		"./polyfills": {
			"default": "./src/lib/polyfills/index.ts"
		},
		"./icons/link.svg": "./src/lib/icons/link.svg",
		"./icons/search.svg": "./src/lib/icons/search.svg",
		"./search": {
			"default": "./src/lib/search/index.ts",
			"svelte": "./src/lib/search/index.ts"
		},
		"./server/content": {
			"default": "./src/lib/server/content/index.ts",
			"svelte": "./src/lib/server/content/index.ts"
		},
		"./styles/base.css": "./src/lib/styles/css/base.css",
		"./styles/code.css": "./src/lib/styles/css/code.css",
		"./styles/fonts.css": "./src/lib/styles/css/fonts.css",
		"./styles/text.css": "./src/lib/styles/css/text.css",
		"./styles/tokens.css": "./src/lib/styles/css/tokens.css",
		"./styles/_css_variables.sass": "./src/lib/styles/_css_variables.sass",
		"./styles/index.sass": "./src/lib/styles/index.sass",
		"./styles/design-system_variables.sass": "./src/lib/styles/design-system_variables.sass"
	},
	"files": [
		"dist",
		"src"
	]
}
