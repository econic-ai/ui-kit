{
	"name": "@econic/ui-kit",
	"version": "1.0.0",
	"sideEffects": false,
	"description": "Styles and components for use in *.econic.ai websites",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"check": "svelte-kit sync && svelte-check",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"lint": "prettier --check .",
		"format": "prettier --write ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/econic-ai/ui-kit.git"
	},
	"author": "Jordan Rancie",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/econic-ai/ui-kit/issues"
	},
	"homepage": "https://github.com/econic-ai/ui-kit/tree/main#readme",
	"dependencies": {
		"@codemirror/autocomplete": "^6.9.0",
		"@codemirror/commands": "^6.2.5",
		"@codemirror/lang-css": "^6.2.1",
		"@codemirror/lang-html": "^6.4.6",
		"@codemirror/lang-javascript": "^6.2.1",
		"@codemirror/language": "^6.9.0",
		"@codemirror/lint": "^6.4.1",
		"@codemirror/search": "^6.5.2",
		"@codemirror/state": "^6.5.0",
		"@codemirror/view": "^6.35.3",
		"@fontsource/inter": "^5.1.1",
		"@lezer/common": "^1.0.4",
		"@replit/codemirror-lang-svelte": "^6.0.0",
		"@shikijs/twoslash": "^1.22.0",
		"autoprefixer": "^10.4.18",
		"esm-env": "^1.0.0",
		"json5": "^2.2.3",
		"postcss": "^8.4.35",
		"sass": "^1.72.0",
		"shiki": "^1.22.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/kit": "^2.12.1",
		"@sveltejs/package": "^2.3.7",
		"@types/node": "^20.12.11",
		"flexsearch": "^0.7.43",
		"magic-string": "^0.30.11",
		"marked": "^14.1.2",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.3.2",
		"publint": "^0.1.9",
		"svelte": "5.14.0",
		"svelte-check": "^4.1.1",
		"typescript": "^5.5.4",
		"vite": "^5.4.3"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^2.12.1",
		"svelte": "^4.0.0 || ^5.0.0-next.1"
	},
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./actions": {
			"types": "./dist/actions/index.d.ts",
			"default": "./dist/actions/index.js"
		},
		"./state": {
			"types": "./dist/state/index.d.ts",
			"default": "./dist/state/index.js"
		},
		"./stores": {
			"types": "./dist/stores/index.d.ts",
			"default": "./dist/stores/index.js"
		},
		"./branding/svelte-logo.svg": "./dist/branding/svelte-logo.svg",
		"./components": {
			"types": "./dist/components/index.d.ts",
			"svelte": "./dist/components/index.js"
		},
		"./docs": {
			"types": "./dist/docs/index.d.ts",
			"svelte": "./dist/docs/index.js"
		},
		"./markdown": {
			"types": "./dist/markdown/index.d.ts",
			"default": "./dist/markdown/index.js"
		},
		"./markdown/preprocess": {
			"types": "./dist/markdown/preprocess.d.ts",
			"default": "./dist/markdown/preprocess.js"
		},
		"./nav": {
			"types": "./dist/nav/index.d.ts",
			"svelte": "./dist/nav/index.js"
		},
		"./polyfills": {
			"types": "./dist/polyfills/index.d.ts",
			"default": "./dist/polyfills/index.js"
		},
		"./icons/link.svg": "./dist/icons/link.svg",
		"./icons/search.svg": "./dist/icons/search.svg",
		"./search": {
			"types": "./dist/search/index.d.ts",
			"svelte": "./dist/search/index.js"
		},
		"./server/content": {
			"types": "./dist/server/content/index.d.ts",
			"default": "./dist/server/content/index.js"
		},
		"./styles/base.css": "./dist/styles/base.css",
		"./styles/code.css": "./dist/styles/code.css",
		"./styles/fonts.css": "./dist/styles/fonts.css",
		"./styles/text.css": "./dist/styles/text.css",
		"./styles/tokens.css": "./dist/styles/tokens.css",
		"./styles/_css_variables.sass": "./dist/styles/_css_variables.sass",
		"./styles/index.sass": "./dist/styles/index.sass",
		"./styles/design-system_variables.sass": "./dist/styles/design-system_variables.sass"
	},
	"files": [
		"dist"
	]
}
